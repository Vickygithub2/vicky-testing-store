<div class="page-width">
  <div class="grid">
    <div class="grid__item medium-up--five-sixths medium--up--push-one-twelfth">
      <div class="section-header text-center">
        <h1>{{ page.title }}</h1>
        <img
          src="{{section.settings.image}}"
          alt="image"
          loading="lazy"
          width="480"
          height="480">
      </div>

      <div class="rte">
        {{ page.content }}

      </div>
    </div>
  </div>
</div>